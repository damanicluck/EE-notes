% adapted from https://tex.stackexchange.com/questions/296463/how-to-create-theorem-boxes
\usepackage{amsmath}
\usepackage{cleveref}
\usepackage[most]{tcolorbox}

\newtcbtheorem[no counter]{Define}{Definition}{
  enhanced,
  sharp corners,
  attach boxed title to top left={
    yshifttext=-1mm
  },
  colback=white,
  colframe=blue!25,
  fonttitle=\bfseries,
  coltitle=black,
  boxed title style={
    sharp corners,
    size=small,
    colback=blue!25,
    colframe=blue!25,
  } 
}{prf}

% copied from : https://tex.stackexchange.com/questions/481532star-wye-electrical-connection-math-symbol
\newcommand\wye{\scalerel*{\stackengine{-1pt}{%
  \rotatebox[origin=c]{30}{\rule{10pt}{.9pt}}\kern-1pt%
  \rotatebox[origin=c]{-30}{\rule{10pt}{1.3pt}}}{%
  \rule{.9pt}{10pt}}{O}{c}{F}{F}{S}}{\Delta}}



